<%-- any content can be specified here e.g.: --%>
<%@ page pageEncoding="UTF-8" %>
<div class="modal-dialog">
    <div class="modal-content shadow border-0">
        <div class="modal-header bg-primary">
            <h5 class="modal-title" id="new_cashout_request">New CashOut Request</h5>
            <button type="button" class="close text-white" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">Ã—</span>
            </button>
        </div>
        <div class="modal-body p-0">
            <div class="mb-0">
                <div class="card-body px-lg-5 py-lg-5">
                    <form name="cashoutForm" id="cashoutForm">
                        <div class="form-group mb-3">
                            <label class="text-center">Amount</label>
                            <div class="input-group input-group-alternative">
                                <div class="input-group-prepend"> 
                                    <span class="input-group-text p-0 w-7">
                                        <i class="fa fa-money mx-auto text-muted fs-18"></i>
                                    </span>
                                </div> 
                                <input class="form-control" id="cashoutAmount" placeholder="Amount" type="text"  data-type="currency">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="text-center">Wallet Pin</label>
                            <div class="input-group input-group-alternative">
                                <div class="input-group-prepend"> 
                                    <span class="input-group-text p-0 w-7">
                                        <i class="fa fa-key mx-auto text-muted fs-18"></i>
                                    </span>
                                </div> 
                                <input class="form-control" id="cashoutPin" placeholder="Wallet Pin" type="password">
                            </div>
                        </div>

                        <div class="text-center"> 
                            <button type="submit" class="btn btn-primary my-4">Submit</button> 
                        </div>
                    </form>
                </div>
            </div>
        </div>
       
    </div>
</div>

